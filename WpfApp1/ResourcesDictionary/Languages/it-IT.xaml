<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">


    <system:String x:Key="LanguageSelection">Seleziona la lingua:</system:String>
    <system:String x:Key="it-IT">it-IT</system:String>
    <system:String x:Key="en-US">en-US</system:String>
    <system:String x:Key="Text1">Descrizione tresto 1</system:String>
    <system:String x:Key="Text2">Descrizione tresto 2</system:String>
    <system:String x:Key="ThemeSelection">Seleziona Tema:</system:String>
    <system:String x:Key="Light">Chiaro</system:String>
    <system:String x:Key="Dark">Scuro</system:String>
</ResourceDictionary>